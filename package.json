{
    "name": "@ifty64bit/enhanced-enum",
    "version": "1.0.0",
    "description": "Type-safe, bi-directional, enum-like structure for TypeScript",
    "homepage": "https://github.com/ifty64bit/enhanced-enum#readme",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup"
    },
    "keywords": [
        "enum",
        "typescript",
        "bi-directional",
        "reverse",
        "typesafe"
    ],
    "author": "Ifty64bit",
    "license": "MIT",
    "devDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    }
}
